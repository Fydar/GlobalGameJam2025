.holodrone-typeselector-foldoutfield {
  flex-direction: row;
  flex-shrink: 0;
  margin-bottom: 1px;
  margin-left: 3px;
  margin-right: -2px;
  margin-top: 1px;
}

  .holodrone-typeselector-foldoutfield Foldout Toggle {
    margin-top: 0;
    margin-bottom: 0;
    margin-left: -16px;
  }

  .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__dropdown {
    background-color: #515151;
    border-width: 1px;
    border-color: #303030;
    border-bottom-color: #242424;
    border-radius: 3px;
    overflow: hidden;
    flex-direction: row;
    flex-basis: 0%;
    padding-top: 0;
    padding-bottom: 1px;
    padding-left: 3px;
    padding-right: 3px;
    flex-shrink: 0;
    flex-grow: 1;
    align-items: center;
  }

    .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__dropdown:hover {
      background-color: #585858;
    }

    .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__dropdown:focus {
      border-color: #3A79BB;
    }

    .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__valueicon {
      width: 16px;
      min-width: 16px;
      height: 16px;
      margin-right: 3px;
    }

    .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__arrow {
    }

  .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__text {
    color: #E4E4E4;
    margin-top: 1px;
    -unity-text-align: middle-left;
    flex-shrink: 1;
    overflow: hidden;
    text-overflow: clip;
    white-space: nowrap;
  }
  

  .holodrone-typeselector-foldoutfield .holodrone-typeselector-foldoutfield__spacer {
    flex-grow: 1;
  }
  
.mixedvalues .holodrone-typeselector-foldoutfield__text {
  color: rgba(210, 210, 210, 0.5);
}
